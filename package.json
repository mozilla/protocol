{
  "name": "protocol",
  "version": "16.0.0",
  "private": true,
  "author": "Mozilla",
  "description": "A design system for Mozilla's websites.",
  "homepage": "https://github.com/mozilla/protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/protocol.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "prestart": "npm install && npm run lint",
    "start": "gulp --dev",
    "webpack": "webpack --config webpack.static.config.js --mode development && webpack --config webpack.build.config.js --mode development",
    "build": "npm run prestart && npm run webpack",
    "build-docs": "npm run prestart && gulp build",
    "lint-css": "./node_modules/.bin/stylelint \"src/assets/sass/**/*.scss\" ",
    "lint-js": "./node_modules/.bin/eslint \"src/assets/js/**/*.js\" \"gulpfile.js/**/*.js\" \"webpack.static.config.js\" \"webpack.build.config.js\" ",
    "lint": "npm run lint-js && npm run lint-css ",
    "test": "./node_modules/.bin/karma start ./tests/karma.conf.js",
    "pretest": "npm run build",
    "readme": "doctoc --title \"### Contents\" --maxlevel 2 docs/README.md",
    "prepublishOnly": "npm run webpack"
  },
  "dependencies": {
    "@cloudfour/hbs-helpers": "^0.9.0",
    "@mozilla-protocol/assets": "^5.1.0",
    "@mozilla-protocol/tokens": "^5.0.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "del": "^6.0.0",
    "drizzle-builder": "0.0.10",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "merge-stream": "^2.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "prismjs": "^1.27.0",
    "require-dir": "^1.2.0",
    "sass": "^1.41.1",
    "sass-loader": "^12.4.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-remove-empty-scripts": "^0.7.1",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "ansi-regex": ">=6.0.1",
    "axios": ">=0.21.2",
    "browser-sync": "^2.27.5",
    "deep-extend": ">=0.5.1",
    "doctoc": "^2.0.1",
    "eslint": "^8.3.0",
    "jasmine-core": "^3.9.0",
    "karma": "^6.3.14",
    "karma-firefox-launcher": "^2.1.1",
    "karma-jasmine": "^4.0.1",
    "postcss-scss": "^4.0.2",
    "set-value": ">=4.0.1",
    "stylelint": "^14.1.0",
    "tar": ">=4.4.18",
    "trim": ">=0.0.3",
    "trim-newlines": ">=3.0.1"
  },
  "browserslist": [
    "defaults",
    "IE 10"
  ]
}
