{
  "name": "protocol",
  "version": "16.0.0",
  "private": true,
  "author": "Mozilla",
  "description": "A design system for Mozilla's websites.",
  "homepage": "https://github.com/mozilla/protocol",
  "license": "MPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/protocol.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "npm run prestart && npm run webpack",
    "fractal-build": "fractal build",
    "lint-css": "./node_modules/.bin/stylelint \"assets/sass/**/*.scss\" \"theme/assets/sass/**/*.scss\" ",
    "lint-js": "./node_modules/.bin/eslint \"assets/js/**/*.js\" \"webpack.static.config.js\" \"webpack.build.config.js\" ",
    "lint": "npm run lint-js && npm run lint-css ",
    "prepublishOnly": "npm run webpack",
    "prestart": "npm install && npm run lint",
    "start": "ENV=development fractal start --sync",
    "webpack": "webpack --config webpack.static.config.js --mode development && webpack --config webpack.build.config.js --mode development"
  },
  "dependencies": {
    "@frctl/fractal": "^1.5.11",
    "@frctl/mandelbrot": "^1.9.4",
    "@frctl/nunjucks": "^2.0.13",
    "@mozilla-protocol/assets": "^5.1.0",
    "@mozilla-protocol/tokens": "^5.0.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^2.4.5",
    "sass-loader": "^12.4.0",
    "sass": "^1.41.1",
    "webpack-cli": "^4.9.1",
    "webpack-remove-empty-scripts": "^0.7.1",
    "webpack": "^5.65.0"
  },
  "devDependencies": {
    "eslint": "^8.3.0",
    "postcss-scss": "^4.0.2",
    "stylelint": "^14.1.0"
  }
}
